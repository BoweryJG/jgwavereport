# Copy this file to .env.local for local development
# These are all free APIs that don't require keys

# NOAA API endpoints (no key required)
REACT_APP_NOAA_API_BASE=https://api.tidesandcurrents.noaa.gov/api/prod
REACT_APP_NWS_API_BASE=https://api.weather.gov

# Open-Meteo Marine API (no key required)
REACT_APP_MARINE_API_BASE=https://marine-api.open-meteo.com/v1/marine

# Update interval in minutes
REACT_APP_UPDATE_INTERVAL=30